; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="osu! skin mixer"
run/main_scene="res://src/Splash.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="osu-skin-mixer"
config/features=PackedStringArray("4.2", "C#")
run/low_processor_mode=true
boot_splash/bg_color=Color(0.109804, 0.105882, 0.121569, 1)
boot_splash/image="res://assets/icon.png"
boot_splash/fullsize=false
config/icon="res://assets/icon.png"

[debug]

file_logging/max_log_files=3

[display]

window/size/viewport_width=750
window/size/viewport_height=500
window/energy_saving/keep_screen_on=false
window/subwindows/embed_subwindows=false
window/dpi/allow_hidpi=false

[dotnet]

project/assembly_name="OsuSkinMixer"

[mono]

project/assembly_name="OsuSkinMixer"

[physics]

common/enable_pause_aware_picking=true

[rendering]

renderer/rendering_method="gl_compatibility"
driver/threads/thread_model=0
environment/defaults/default_clear_color=Color(0.109804, 0.105882, 0.121569, 1)
quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=1
quality/intended_usage/framebuffer_allocation.mobile=1
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color(0.729412, 0.615686, 0.756863, 1)
environment/default_environment="res://default_env.tres"
